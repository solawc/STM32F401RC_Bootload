{
    "build": {
      "core": "stm32",
      "cpu": "cortex-m4",
      "extra_flags": "-DSTM32G473xx -DSTM32G4xx",
      "f_cpu": "170000000L",
      "framework_extra_flags": {
        "arduino": "-DCUSTOM_PERIPHERAL_PINS"
      },
      "mcu": "stm32g473rct6",
      "product_line": "STM32G473xx",
      "variant": "STM32G4xx"
    },
    "debug": {
      "default_tools": [
        "jlink"
      ],
      "jlink_device": "STM32G473RC",
      "openocd_target": "stm32g4x",
      "svd_path": "STM32G473xx.svd"
    },
    "frameworks": [
      "arduino",
      "cmsis",
      "stm32cube"
    ],
    "name": "STM32G473RC (128k RAM. 256k Flash)",
    "upload": {
      "maximum_ram_size": 131027,
      "maximum_size": 262144,
      "protocol": "jlink",
      "protocols": [
        "jlink",
        "stlink",
        "cmsis-dap"
      ]
    },
    "url": "https://www.st.com/en/microcontrollers-microprocessors",
    "vendor": "Generic"
  }
  